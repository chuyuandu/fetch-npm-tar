  "dependencies": {
    "arg": "^5.0.2",
    "chalk": "^5.4.1",
    "inquirer": "^12.6.0",
    "ora": "^8.2.0",
    "p-limit": "^6.2.0",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/node": "20",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.3",
    "vitest": "^3.1.2"
