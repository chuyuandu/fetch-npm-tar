{
  "name": "fetch-npm-tar",
  "version": "1.2.1",
  "description": "download npm tar and all dependencies",
  "main": "dist/fetch-npm-tar.mjs",
  "module": "dist/fetch-npm-tar.mjs",
  "type": "module",
  "bin": {
    "fetch-npm-tar": "./dist/fetch-npm-tar.bin.mjs"
  },
  "keywords": [
